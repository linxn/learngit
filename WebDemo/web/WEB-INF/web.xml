         version="3.1">
    <servlet>
        <servlet-name>litTest</servlet-name>
        <servlet-class>com.linxn.litTest.litTest</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>litTest</servlet-name>
